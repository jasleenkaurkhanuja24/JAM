<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Signupservlet</servlet-name>
        <servlet-class>com.mycompany.youtubee.servlets.Signupservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Loginservlet</servlet-name>
        <servlet-class>com.mycompany.youtubee.servlets.Loginservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPostServlet</servlet-name>
        <servlet-class>com.mycompany.youtubee.servlets.AddPostServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProfileServlet</servlet-name>
        <servlet-class>com.mycompany.youtubee.servlets.EditProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletingservlet</servlet-name>
        <servlet-class>com.mycompany.youtubee.servlets.deletingservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>likeservlet</servlet-name>
        <servlet-class>com.mycompany.youtubee.servlets.likeservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>commentservlet</servlet-name>
        <servlet-class>com.mycompany.youtubee.servlets.commentservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signupservlet</servlet-name>
        <url-pattern>/Signupservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Loginservlet</servlet-name>
        <url-pattern>/Loginservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPostServlet</servlet-name>
        <url-pattern>/AddPostServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfileServlet</servlet-name>
        <url-pattern>/EditProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletingservlet</servlet-name>
        <url-pattern>/deletingservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>likeservlet</servlet-name>
        <url-pattern>/likeservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>commentservlet</servlet-name>
        <url-pattern>/commentservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
